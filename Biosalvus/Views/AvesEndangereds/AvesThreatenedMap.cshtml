@model Biosalvus.Models.EndageredMapViewModel
@{
    ViewBag.Title = "Threat: Feral Cats";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style>
    .legend {
        background-color: #fff;
        border-radius: 3px;
        bottom: 30px;
        box-shadow: 0 1px 2px rgba(0, 0, 0, 0.1);
        font: 12px/20px 'Helvetica Neue', Arial, Helvetica, sans-serif;
        padding: 10px;
        right: 10px;
        z-index: 1;
    }

        .legend h4 {
            margin: 0 0 10px;
        }

        .legend div span {
            border-radius: 50%;
            display: inline-block;
            height: 10px;
            margin-right: 5px;
            width: 10px;
        }

    .list {
        cursor: pointer;
        font-size: 12px;
        padding-left: 20px
    }

        .list:hover {
            background-color: lightblue;
        }

    .dropdown-header {
        padding-left: 0px;
    }

    .button {
        border-radius: 4px;
        margin-bottom:20px;
    }

        .button:hover {
            background-color: #4c956c;
            color: white;
        }
</style>
<header class="section background-dark">
    <div class="line">
        <h1 class="text-white margin-top-bottom-40 text-size-60 text-line-height-1">Feral Cats</h1>
        <p class="margin-bottom-0 text-size-16">
            Explore our Dashboard to view the Threatened Birds vs. Feral Cats occurances.<br>
            You can start by selecting either the Threatened Status of the birds.<br>
            Distinct Birds details are selectable from the dropdown.
        </p>
    </div>
</header>
<h2 class="text-size-50 text-center">@ViewBag.Title</h2>
<hr class="break-small background-primary break-center">
<div class="row">
    <div class="col-md-10">
        <div id="map" style="height:450px;"></div> <!--This is the Map-->
    </div>
    <div class="col-md-2">
        <div class="row">
            <strong>Select bellow Threatened Status to Start</strong>
            <div class="col-md-12 col-xs-12">
                <button class="button" id="vulnerablebtn" style="font-size:large;width:100%;height:50px;color:white;background-color:#2c6e49;">
                    Vulnerable
                </button>
            </div>
            <div class="col-md-12 col-xs-12">
                <button class="button" id="endangeredbtn" style="font-size:large;width:100%;height:50px;color:white;background-color:#2c6e49;">
                    Endangered
                </button>
            </div>
            <div class="col-md-12 col-xs-12">
                <button class="button" id="critendangeredbtn" style="font-size:large;width:100%;height:50px;color:white;background-color:#2c6e49;">
                    Critically Endangered
                </button>
            </div>
            <strong>Search a specific bird from the list bellow</strong>
            <div class="col-md-12 col-xs-12">
                <div class="dropdown">
                    <button class="btn btn-default dropdown-toggle" type="button" data-toggle="dropdown" style="width:100%;">
                        Birds List
                        <span class="caret"></span>
                    </button>
                    <ul class="dropdown-menu">
                        <li id="vulnerableheader" class="dropdown-header"><strong>Vulnerable</strong></li>
                        <li id="nalst" class="list">N/A</li>
                        <li id="endangeredheader" class="dropdown-header"><strong>Endangered</strong></li>
                        <li id="easternbristlebirdlst" class="list">Eastern Bristlebird</li>
                        <li id="easterngroundparrotlst" class="list">Eastern Ground Parrot</li>
                        <li id="helmetedhoneyeaterlst" class="list">Helmeted Honeyeater</li>
                        <li id="hoodedrobinlst" class="list">Hooded Robin</li>
                        <li id="southernemuwrenlst" class="list">Southern Emu-wren</li>
                        <li id="southerngiantpetrellst" class="list">southern giant-petrel</li>
                        <li id="swiftparrotlst" class="list">Swift Parrot</li>
                        <li id="yellowtuftedhoneyeaterlst" class="list">Yellow-tufted Honeyeater</li>
                        <li id="critendangeredheader" class="dropdown-header"><strong>Critically Endangered.</strong></li>
                        <li id="orangebelliedparrotlst" class="list">Orange-bellied Parrot</li>
                        <li id="spottedquailthrushlst" class="list">Spotted Quail-thrush</li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
</div>

@*Eastern Bristlebird*@
<div id="easternbristlebird" style="display:none;">
    @foreach (var item in Model.easternBristlebirdRankings.Take(1))
    {<h3><strong>@Html.DisplayFor(modelitem => item.speciesname)</strong></h3>}
    <div class="row">
        <div class="col-md-6">
            <img class="img-responsive" src="~/Content/Images/Birds/eastern-bristlebird.jpg" alt="easternbristlebird" />
            The eastern bristlebird is a species of bird in the bristlebird family, Dasyornithidae. It is endemic to Australia. Its natural habitats are temperate forests, temperate shrubland, and temperate grassland. It is threatened by habitat loss.
        </div>
        <div class="col-md-6">
            <table class="table">
                <tr><th data-toggle="tooltip" title="Total historal count from data source">Bird Count</th><th>Status</th><th data-toggle="tooltip" title="Registered PSMA Administrative Boundaries">City</th><th data-toggle="tooltip" title="Count Per City/Total Count">Cat Rate</th></tr>
                @foreach (var item in Model.easternBristlebirdRankings.Take(5))
                {
                    <tr class="birddata">
                        <td class="birdcount">@Html.DisplayFor(modelitem => item.birdcount)</td>
                        <td class="birdstatus">@Html.DisplayFor(modelitem => item.status)</td>
                        <td class="birdcity">@Html.DisplayFor(modelitem => item.locality)</td>
                        <td class="threatrate">@Html.DisplayFor(modelitem => item.threatrate)</td>
                    </tr>
                }
            </table>
        </div>
    </div>
</div>
